

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WAEC School Recognition System</title>

    <link rel="stylesheet" href="~/lib/fontawesome-free/css/all.min.css">
    <!-- icheck bootstrap -->
    <link rel="stylesheet" href="~/lib/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/css/waeclte.min.css">
    <link href="~/lib/toastr/toastr.min.css" rel="stylesheet" />

    @RenderSection("styles", required: false)
    <link href="~/css/custom.css" rel="stylesheet" />
</head>
<body class="hold-transition login-page bg-login">


    @RenderBody()

    <footer class="border-top footer text-muted">
        <div class="container text-black-50">
            &copy; @DateTime.Now.Year - SchoolRecognition - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/js/waeclte.min.js" asp-append-version="true"></script>
    <script src="~/lib/toastr/toastr.min.js"></script>
    @RenderSection("Scripts", required: false)
    @await Component.InvokeAsync("NToastNotify")
</body>
</html>
